<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dong.study">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".support.SupportActivity" />
        <activity android:name=".support.SwipeRefreshActivity" />
        <activity android:name=".support.RecyclerViewActivity" />
        <activity android:name=".support.RecyclerViewHeaderFooterActivity" />
        <activity android:name=".support.RecyclerViewItemTouchHelperActivity" />

    </application>

</manifest>